{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ChangeMonitor": "Host=<host>; Username=<user>; Password=<password>; Database=<db_name>;"
  },
  "ChangeMonitor": {
    "Resources": [
      {
        "DisplayName": "GitHub",
        "Description": "GitHub targets",
        "Targets": [
          {
            "DisplayName": "GitHub profile title",
            "Description": "GitHub profile title text",
            "Url": "https://github.com/equenum?tab=repositories",
            "CronSchedule": "0/5 * * * * ?",
            "Type": "ValueCheck",
            "HtmlTag": "span",
            "Selector": {
              "Type": "Class",
              "Value": "p-name vcard-fullname d-block overflow-hidden"
            },
            "ExpectedValue": ""
          }
        ]
      }
    ]
  }
}
