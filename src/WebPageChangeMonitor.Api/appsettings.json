{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ChangeMonitor": "Host=<host>; Username=<user>; Password=<password>; Database=<db_name>;"
  },
  "ChangeMonitor": {
    "DefaultResourcePageSize": 10,
    "DefaultTargetPageSize": 10,
    "DefaultTargetSnapshotPageSize": 10,
    "BackoffType": 2,
    "UseJitter": true,
    "JobRetry": {
      "MaxAttempts": 3,
      "Delay": "00:00:05"
    },
    "Notifications": {
      "OriginName": "web-change-monitor-api",
      "AreEnabled": false,
      "Channels": [
        {
          "Name": "Telegram",
          "IsEnabled": false,
          "Url": "<url>",
          "TenantId": "<tenant-id>",
          "TenantToken": "<tenant-token>",
          "TenantIdHeader": "<tenant-id-header>",
          "TenantTokenHeader": "<tenant-token-header>"
        },
        {
          "Name": "PocPrintHub",
          "IsEnabled": false,
          "Url": "<url>",
          "TenantId": "<tenant-id>",
          "TenantToken": "<tenant-token>",
          "TenantIdHeader": "Pph-Tenant-Id",
          "TenantTokenHeader": "Pph-Tenant-Token"
        }
      ]
    }
  }
}
